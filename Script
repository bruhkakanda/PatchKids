-- StarterGui.PK_API.mainframe.Loader
local function C_4()
local script = G2L["4"];
	local UserInputService = game:GetService("UserInputService")
	wait(2)
	local newwindow = require(script.NewWindow)
	local NewNotification = require(script.NewNotification)
	local tab = require(script.NewTab)
	
	local universal = require(script.Universal)
	
	NewNotification.New("Welcome to PatchKids!")
	
	UserInputService.InputBegan:Connect(function(input, p)
		if not p then
			if input.KeyCode == Enum.KeyCode.Q then
				script.Parent.Visible = not script.Parent.Visible
				if script.Parent.Visible then
					script.Parent.UIScale.Scale = 1
				else
					script.Parent.UIScale.Scale = 0
				end
			end
		end
	end)
	
	universal.Start()
	
	if script.Gamesloader:FindFirstChild(game.PlaceId) then
		for i,v in script.Gamesloader:GetChildren() do
			if v.Name ~= tostring(game.PlaceId) then
				v:Destroy()
			end
		end
		
		local r = require(script.Gamesloader:FindFirstChild(game.PlaceId))
		r.Start()
	else
		for i,v in script.Gamesloader:GetChildren() do
			v:Destroy()
		end
	end
end;
task.spawn(C_4);
