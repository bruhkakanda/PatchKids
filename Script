G2L_MODULES[G2L["39"]] = {
Closure = function()
    local script = G2L["39"];local newwindow = require(script.Parent.Parent.NewWindow)
local NewNotification = require(script.Parent.Parent.NewNotification)
local tab = require(script.Parent.Parent.NewTab)

-- Esp Stuff
local Rep = game.ReplicatedStorage

local EspModule = require(script.Esp)
local AFModule = require(script.Autofarm)
local shootModule = require(script.shoot)
-- End
-- Cham Stuff

local db = false
local toggleEsp = false
local toggleAF = false

local module = {}

local function getClosestPlayer()
	local Players = game:GetService("Players")
	local localPlayer = Players.LocalPlayer
	local character = localPlayer.Character
	if not character then return nil end

	local root = character:WaitForChild("HumanoidRootPart")

	local closestPlayer = nil
	local closestDistance = math.huge

	for _, player in ipairs(Players:GetPlayers()) do
		if player ~= localPlayer and player.Character then
			local humanoid = player.Character:FindFirstChildOfClass("Humanoid")
			local hrp = player.Character:FindFirstChild("HumanoidRootPart")

			if humanoid and hrp and humanoid.Health > 0 then
				local distance = (hrp.Position - root.Position).Magnitude

				if distance < closestDistance then
					closestDistance = distance
					closestPlayer = player
				end
			end
		end
	end

	return closestPlayer, closestDistance
end

local function findMurderer(roles)
	for i, v in roles do
		if v.Role and v.Role == "Murderer" then
			if game.Players:FindFirstChild(i) then
				return game.Players[i]
			end
		end
	end

	return nil
end

function module.Start()

	local newTab = tab.New("Main")

	newwindow.NewTitle("Visuals", "Main")
	local Esp = newwindow.NewToggle("Player Esp!","esp","Main")

	newwindow.NewTitle("Sheriff", "Main")
	local grabgun = newwindow.NewWindow("Grab Gun", "gun grabber", "Main")
	local TPSHOOT = newwindow.NewWindow("TP Shoot Murderer", "", "Main")

	newwindow.NewTitle("Murderer", "Main")
	local killclosest1 = newwindow.NewWindow("Kill closest player", "", "Main")
	local KILLALL = newwindow.NewWindowV2("Kill all", "", "Main", function()
		local roles = game.ReplicatedStorage:FindFirstChild("GetPlayerData", true):InvokeServer()
		local yourData = roles[game.Players.LocalPlayer.Name]
		local root = game.Players.LocalPlayer.Character.PrimaryPart

		if yourData then
			local yourRole = yourData.Role
			if yourRole == "Murderer" then
				local isInBackpack = game.Players.LocalPlayer.Backpack:FindFirstChild("Knife")
				if isInBackpack then
					isInBackpack.Parent = game.Players.LocalPlayer.Character
				end
				
					for i, v in game.Players:GetPlayers() do
						if v ~= game.Players.LocalPlayer then
							local args = {
						v.Character.PrimaryPart
					}
					game.Players.LocalPlayer.Character.Knife.Events.KnifeStabbed:FireServer()
					game.Players.LocalPlayer.Character.Knife.Events.HandleTouched:FireServer(unpack(args))
						end
					end
			else
				NewNotification.New("You are not the murderer!", 1.5)
			end
		end
	end)

	newwindow.NewTitle("Teleports", "Main")
	local MapTP = newwindow.NewWindow("Teleport to map", "", "Main")
	local LobbyTP = newwindow.NewWindow("Teleport to lobby", "", "Main")

	local premiumUsers = loadstring(game:HttpGet("https://raw.githubusercontent.com/bruhkakanda/PatchKids/refs/heads/main/config"))()
	if table.find(premiumUsers, game.Players.LocalPlayer.Name) then
			local premiumTab = tab.New("Premium")

			newwindow.NewTitle("- Premium Sheriff -", "Premium")

			local premiumGrabber = newwindow.NewWindowV2("Fast Gun Grab", "gun grabber", "Premium", function()
				local gun = workspace:FindFirstChild("GunDrop", true)
				if gun then
					firetouchinterest(game.Players.LocalPlayer.Character.PrimaryPart, gun, 0)
					task.wait(0.1)
					local hasGunNow = game.Players.LocalPlayer.Backpack:FindFirstChild("Gun")
					if hasGunNow then
						NewNotification.New("Successfuly Grabbed GunDrop!", 1.5)
					end
				else
					NewNotification.New("GunDrop not found!", 1)
				end
			end)

			local premiumGrabberButton = newwindow.NewButton("Fast Gun Grab")

			newwindow.NewTitle("- Premium Murderer -", "Premium")

			local silentKillClosest = newwindow.NewWindowV2("Silent Kill Closest", "", "Premium", function()
				local closestTarget = getClosestPlayer()
				if closestTarget then
					if game.Players.LocalPlayer.Backpack:FindFirstChild("Knife") then
							local args = {
							closestTarget.Character.PrimaryPart
						}
						game.Players.LocalPlayer.Backpack.Knife.Events.KnifeStabbed:FireServer()
						game.Players.LocalPlayer.Backpack.Knife.Events.HandleTouched:FireServer(unpack(args))
					else
						NewNotification.New("Unequip your knife!")
					end
				end
			end)

			newwindow.NewTitle("- Fun -", "Premium")

			local flingMurderer = newwindow.NewWindow("Fling Murderer", "", "Premium")
				
			newwindow.NewTitle("Autofarm", "Premium")

			local Autofarm = newwindow.NewToggle("Autofarm","Autofarm coins!","Premium")

			task.spawn(function()
				AFModule.Start()
			end)

			Autofarm.Activated:Connect(function()
				if toggleAF then
					toggleAF = false
					Autofarm.Toggle.Value = false
					AFModule.toggle(false)
				else
					Autofarm.Toggle.Value = true
					AFModule.toggle(true)
					toggleAF = true
				end
			end)

			flingMurderer.Activated:Connect(function()
				NewNotification.New("You can't access this feature or this feature is not completed yet!", 1)
			end)

			premiumGrabberButton.Activated:Connect(function()
				local gun = workspace:FindFirstChild("GunDrop", true)
				if gun then
					firetouchinterest(game.Players.LocalPlayer.Character.PrimaryPart, gun, 0)
					task.wait(0.1)
					local hasGunNow = game.Players.LocalPlayer.Backpack:FindFirstChild("Gun")
					if hasGunNow then
						NewNotification.New("Successfuly Grabbed GunDrop!", 1.5)
					end
				else
					NewNotification.New("GunDrop not found!", 1)
				end
			end)
	end
	
	local silentAim = newwindow.NewButton("Shoot Murderer")
	local killclosestButton = newwindow.NewButton("Kill closest ðŸ”ª")

	if not table.find(premiumUsers, game.Players.LocalPlayer.Name) then
		local int_gun = newwindow.NewButton("Grab Gun ðŸ”«")
		int_gun.Activated:Connect(function()
			if workspace:FindFirstChild("GunDrop", true) then
				local lastPos = game.Players.LocalPlayer.Character.PrimaryPart.CFrame
				game.Players.LocalPlayer.Character:FindFirstChild("Humanoid").Jump = true
				game.Players.LocalPlayer.Character:PivotTo(workspace:FindFirstChild("GunDrop", true).CFrame)
				task.wait(0.1)
				game.Players.LocalPlayer.Character:PivotTo(lastPos)
				NewNotification.New("Grabbed Gun!", 1.5)
			else
				NewNotification.New("No gun found!", 1.5)
			end
		end)
	end

	Esp.Activated:Connect(function()
		if db == false then
			db = true
			if Esp.Toggle.Value == false then
				Esp.Toggle.Value = true
				toggleEsp = true
				wait(.1)
				db = false
			elseif Esp.Toggle.Value == true then
				toggleEsp = false
				Esp.Toggle.Value = false
				wait(.1)
				db = false
			end
		end
	end)

	LobbyTP.Activated:Connect(function()
		local lobby = workspace:FindFirstChild("Lobby") or workspace:FindFirstChild("RegularLobby")
		if lobby then
			local spawns = lobby:FindFirstChild("Spawns")
			if spawns then
				local rCF = spawns:GetChildren()[math.random(1, #spawns:GetChildren())].CFrame + Vector3.new(0,2,0)
				game.Players.LocalPlayer.Character:PivotTo(rCF)
			else
				NewNotification.New("Parts not found!", 1.5)
			end
		else
			NewNotification.New("Lobby not found!", 1.5)
		end
	end)

	MapTP.Activated:Connect(function()
		local map = workspace:FindFirstChild("CoinContainer", true) or workspace:FindFirstChild("CoinAreas", true)
		if map then
			local spawns = map.Parent:FindFirstChild("Spawns")
			if spawns then
				local rCF = spawns:GetChildren()[math.random(1, #spawns:GetChildren())].CFrame + Vector3.new(0,2,0)
				game.Players.LocalPlayer.Character:PivotTo(rCF)
			else
				NewNotification.New("Parts not found!", 1.5)
			end
		else
			NewNotification.New("Map not found!", 1.5)
		end
	end)
	
	grabgun.Activated:Connect(function()
		if workspace:FindFirstChild("GunDrop", true) then
			local lastPos = game.Players.LocalPlayer.Character.PrimaryPart.CFrame
			game.Players.LocalPlayer.Character:FindFirstChild("Humanoid").Jump = true
			game.Players.LocalPlayer.Character:PivotTo(workspace:FindFirstChild("GunDrop", true).CFrame)
			task.wait(0.1)
			game.Players.LocalPlayer.Character:PivotTo(lastPos)
			NewNotification.New("Grabbed Gun!", 1.5)
		else
			NewNotification.New("No gun found!", 1.5)
		end
	end)

	killclosestButton.Activated:Connect(function()
		local roles = game.ReplicatedStorage:FindFirstChild("GetPlayerData", true):InvokeServer()
		local yourData = roles[game.Players.LocalPlayer.Name]
		local root = game.Players.LocalPlayer.Character.PrimaryPart

		if yourData then
			local yourRole = yourData.Role
			if yourRole == "Murderer" then
				local isInBackpack = game.Players.LocalPlayer.Backpack:FindFirstChild("Knife")
				if isInBackpack then
					isInBackpack.Parent = game.Players.LocalPlayer.Character
				end
				local closestTarget = getClosestPlayer()
				if closestTarget then
					local args = {
						closestTarget.Character.PrimaryPart
					}
					game.Players.LocalPlayer.Character.Knife.Events.KnifeStabbed:FireServer()
					game.Players.LocalPlayer.Character.Knife.Events.HandleTouched:FireServer(unpack(args))

				end
			else
				NewNotification.New("You are not the murderer!", 1.5)
			end
		end
	end)

	killclosest1.Activated:Connect(function()
		local roles = game.ReplicatedStorage:FindFirstChild("GetPlayerData", true):InvokeServer()
		local yourData = roles[game.Players.LocalPlayer.Name]
		local root = game.Players.LocalPlayer.Character.PrimaryPart

		if yourData then
			local yourRole = yourData.Role
			if yourRole == "Murderer" then
				local isInBackpack = game.Players.LocalPlayer.Backpack:FindFirstChild("Knife")
				if isInBackpack then
					isInBackpack.Parent = game.Players.LocalPlayer.Character
				end
				local closestTarget = getClosestPlayer()
				if closestTarget then
					local args = {
						closestTarget.Character.PrimaryPart
					}
					game.Players.LocalPlayer.Character.Knife.Events.KnifeStabbed:FireServer()
					game.Players.LocalPlayer.Character.Knife.Events.HandleTouched:FireServer(unpack(args))
				end
			else
				NewNotification.New("You are not the murderer!", 1.5)
			end
		end
	end)

	TPSHOOT.Activated:Connect(function()
		local roles = game.ReplicatedStorage:FindFirstChild("GetPlayerData", true):InvokeServer()
		local yourData = roles[game.Players.LocalPlayer.Name]
		local root = game.Players.LocalPlayer.Character
		local player = game.Players.LocalPlayer

		if yourData then
			local yourRole = yourData.Role
			if yourRole == "Sheriff" or yourRole == "Hero" then
				local murderer = findMurderer(roles)
				if murderer then
					local lastPos = root.PrimaryPart.CFrame
					local targetPrimary = murderer.Character.PrimaryPart
					root:PivotTo(targetPrimary.CFrame * CFrame.new(0,0,4))
					local args = {}
					args[1] = root.RightHand.CFrame
					args[2] = targetPrimary.CFrame

					local backpackGun = player.Backpack:FindFirstChild("Gun")
					if backpackGun then
						backpackGun.Parent = root
					end

					local gun = root:FindFirstChild("Gun")
					if gun then
						gun.Shoot:FireServer(unpack(args))

						task.wait(0.2)
						root.PrimaryPart:PivotTo(lastPos)
					end
				end
			end
		end
	end)
	
	silentAim.Activated:Connect(function()
		shootModule.Shoot()
	end)
	
	
	while task.wait(1) do
		if toggleEsp then
			

				EspModule.CreateLabel()
				EspModule.UpdateLabel()
				EspModule.ForceCreate("GunDrop")

		else
			EspModule.ClearAll()
			end
	end
end

return module
end;
};

G2L_MODULES[G2L["385"]] = {
Closure = function()
    local script = G2L["385"];local newwindow = require(script.Parent.Parent.NewWindow)
local NewNotification = require(script.Parent.Parent.NewNotification)
local tab = require(script.Parent.Parent.NewTab)

-- Esp Stuff
local Rep = game.ReplicatedStorage
-- End
-- Cham Stuff

local module = {}

local function getClosestPlayer()
	local Players = game:GetService("Players")
	local localPlayer = Players.LocalPlayer
	local character = localPlayer.Character
	if not character then return nil end

	local root = character:WaitForChild("HumanoidRootPart")

	local closestPlayer = nil
	local closestDistance = math.huge

	for _, player in ipairs(Players:GetPlayers()) do
		if player ~= localPlayer and player.Character then
			local humanoid = player.Character:FindFirstChildOfClass("Humanoid")
			local hrp = player.Character:FindFirstChild("HumanoidRootPart")

			if humanoid and hrp and humanoid.Health > 0 then
				local distance = (hrp.Position - root.Position).Magnitude

				if distance < closestDistance then
					closestDistance = distance
					closestPlayer = player
				end
			end
		end
	end

	return closestPlayer, closestDistance
end


function module.Start()

	local newTab = tab.New("Main")
	local shootClosest = newwindow.NewWindow("Shoot Closest - R", "gun grabber", "Main") -- FireWeapon(spotfrom, hit)
	local killclosest1 = newwindow.NewWindow("Knife Closest", "", "Main") -- HitEvent
	
	local shootClosestButton = newwindow.NewButton("Shoot Closest")
	local killclosestButton = newwindow.NewButton("Kill closest ðŸ”ª")
	local player = game.Players.LocalPlayer
	local root = player.Character.PrimaryPart

	game.UserInputService.InputBegan:Connect(function(key)
		if key.KeyCode == Enum.KeyCode.R then
			local hasGunInBackpack = player.Backpack:FindFirstChild("FireWeapon", true)
		if hasGunInBackpack then
			hasGunInBackpack.Parent.Parent = player.Character
		end

		local closest = getClosestPlayer()
		if closest then

			local args = {}
			args[1] = vector.create(closest.Character.UpperTorso.Position.X,closest.Character.UpperTorso.Position.Y,closest.Character.UpperTorso.Position.Z)
			args[2] = closest.Character.Head

			player.Character:FindFirstChild("FireWeapon", true):FireServer(unpack(args))
		end
		end
	end)
	
	shootClosest.Activated:Connect(function()
		local hasGunInBackpack = player.Backpack:FindFirstChild("FireWeapon", true)
		if hasGunInBackpack then
			hasGunInBackpack.Parent.Parent = player.Character
		end

		local closest = getClosestPlayer()
		if closest then

			local args = {}
			args[1] = vector.create(closest.Character.UpperTorso.Position.X,closest.Character.UpperTorso.Position.Y,closest.Character.UpperTorso.Position.Z)
			args[2] = closest.Character.Head

			player.Character:FindFirstChild("FireWeapon", true):FireServer(unpack(args))
		end
	end)
	shootClosestButton.Activated:Connect(function()
		local hasGunInBackpack = player.Backpack:FindFirstChild("FireWeapon", true)
		if hasGunInBackpack then
			hasGunInBackpack.Parent.Parent = player.Character
		end

		local closest = getClosestPlayer()
		if closest then

			local args = {}
			args[1] = vector.create(closest.Character.UpperTorso.Position.X,closest.Character.UpperTorso.Position.Y,closest.Character.UpperTorso.Position.Z)
			args[2] = closest.Character.Head

			player.Character:FindFirstChild("FireWeapon", true):FireServer(unpack(args))
		end
	end)


	killclosest1.Activated:Connect(function()
		local hasGunInBackpack = player.Backpack:FindFirstChild("HitEvent", true)
		if hasGunInBackpack then
			hasGunInBackpack.Parent.Parent = player.Character
		end

		local closest = getClosestPlayer()
		if closest then
			local targetCFrame = CFrame.new(
					root.Position + (root.CFrame.LookVector * 2),
					root.Position + (root.CFrame.LookVector * (2 + 1))
					)

					closest.Character:PivotTo(targetCFrame)
			player.Character:FindFirstChild("HitEvent", true):FireServer()
		end
	end)

	killclosestButton.Activated:Connect(function()
		local hasGunInBackpack = player.Backpack:FindFirstChild("HitEvent", true)
		if hasGunInBackpack then
			hasGunInBackpack.Parent.Parent = player.Character
		end

		local closest = getClosestPlayer()
		if closest then
			local targetCFrame = CFrame.new(
					root.Position + (root.CFrame.LookVector * 2),
					root.Position + (root.CFrame.LookVector * (2 + 1))
					)

					closest.Character:PivotTo(targetCFrame)
			player.Character:FindFirstChild("HitEvent", true):FireServer()
		end
	end)
end

return module
end;
};

G2L_MODULES[G2L["3a"]] = {
Closure = function()
    local script = G2L["3a"];local module = {Toggle = false}
local NewNotification = require(script.Parent.Parent.Parent.NewNotification)

local TOGGLE = false
local noclip = false
local LOCKED_Y = nil
local canReset = true

local SPEED = 20

local NO_COIN_TIMEOUT = 5
local lastCoinSeen = os.clock()

local humanoid = game.Players.LocalPlayer.Character:WaitForChild("Humanoid")

module.toggle = function(toggle)
	TOGGLE = toggle
	if toggle then
		noclip = true
		workspace.Gravity = 0
		humanoid.PlatformStand = true
	else
		noclip = false
		workspace.Gravity = 196.2
		humanoid.PlatformStand = false
	end
end

game["Run Service"].Stepped:Connect(function()
	if noclip then
		for _, v in game.Players.LocalPlayer.Character:GetDescendants() do
			if v:IsA("BasePart") then
				v.CanCollide = false
			end
		end
	end
end)


local function getClosestCoin(root)
	local closestCoin = nil
	local closestDist = 1000

	local CoinContainer = workspace:FindFirstChild("CoinContainer", true)

	if CoinContainer then
		for _, v in CoinContainer:GetChildren() do
			local coin = v:FindFirstChild("CoinVisual")
			if coin and coin:FindFirstChild("MainCoin") then
				local dist = (coin.Position - root.Position).Magnitude
				if dist < closestDist then
					closestDist = dist
					closestCoin = coin
				end
			end
		end
	end

	return closestCoin, closestDist
end

module.Start = function()
	while task.wait(0.2) do
		if TOGGLE then
			local char = game.Players.LocalPlayer.Character
			if char and char.Parent and char:FindFirstChild("Humanoid") then
			local humanoid = char:WaitForChild("Humanoid")
			local root = char.PrimaryPart
			

			local coinVisual, distance = getClosestCoin(root)
			local coin = nil

			if coinVisual then
				if coinVisual.Parent then
					if coinVisual:FindFirstChild("MainCoin") then
						coin = coinVisual.Parent
					end
				end
			end

			if not coinVisual or not coinVisual.Parent then

				if os.clock() - lastCoinSeen < NO_COIN_TIMEOUT then
					continue
				else
					if canReset then
						noclip = false
						humanoid.PlatformStand = false
						workspace.Gravity = 196.2
						humanoid.Health = 0
						canReset = false
					end
				end
			else
				canReset = true
				lastCoinSeen = os.clock()
				noclip = true
				humanoid.PlatformStand = true
				workspace.Gravity = 0
			end

	
			lastCoinSeen = os.clock()

			if coin and coin.Parent then
				
				if not LOCKED_Y then
					LOCKED_Y = root.Position.Y
				end

				local targetY = coin.Position.Y - 2.5
				if math.abs(targetY - LOCKED_Y) > 0.1 then
					LOCKED_Y = targetY -- move Y to coin's Y
				end
				local targetCFrame =
					CFrame.new(coin.Position.X, LOCKED_Y, coin.Position.Z) *
					CFrame.Angles(math.rad(90), 0, 0)

				while true do
					local dt = task.wait(0.01)

					if not coinVisual or not coinVisual.Parent then
						break
					end

					if coinVisual:FindFirstChild("MainCoin") then
						if coinVisual.MainCoin.Transparency ~= 0 then
							break
						end
					end

					if not TOGGLE then
						break
					end

					local CoinContainers = workspace:FindFirstChild("CoinContainer", true)
					if CoinContainers then
						for _, coinAura in CoinContainers:GetChildren() do
							if coinAura and coinAura:FindFirstChild("CoinVisual") and coinAura:FindFirstChild("CoinVisual"):FindFirstChild("MainCoin") then
								if coinAura.CoinVisual.MainCoin.Transparency == 0 then
									firetouchinterest(root, coinAura, 0)
								end
							end
						end
					end

					local currentPos = root.Position
					local targetPos = Vector3.new(
						coin.Position.X,
						LOCKED_Y,
						coin.Position.Z
					)

					local direction = targetPos - currentPos
					local dist = direction.Magnitude

					if dist > 0 then
						local step = math.min(SPEED * dt, dist)
						local newPos = currentPos + direction.Unit * step
						root.CFrame = CFrame.new(newPos) * CFrame.Angles(math.rad(90), 0, 0)
					end

					if dist < 1.5 then
						break
					end
				end
			end
		end
		else
			workspace.Gravity = 196.2
			noclip = false
			local char = game.Players.LocalPlayer.Character
			if char and char:FindFirstChild("Humanoid") then
				char.Humanoid.PlatformStand = false
			end
			LOCKED_Y = nil
		end
	end
end


return module

end;
};
G2L_MODULES[G2L["3b"]] = {
Closure = function()
    local script = G2L["3b"];local module = {}


local function IsAlive(v)
		
if not v.Killed and not v.Dead then
			return true
		else
			return nil
		end
end

function module.CreateLabel()
		for i, v in pairs(game.Players:GetChildren()) do
		if (v ~= game.Players.LocalPlayer and v and v.Parent and v.Character and v.Character:FindFirstChild("Head")) and not v.Character:WaitForChild("Head"):FindFirstChild("BillboardGui") then
				local billboard = Instance.new("BillboardGui", v.Character.Head)
				billboard.Size = UDim2.new(4,0,1,0)
				billboard.ExtentsOffset = Vector3.new(0, 2, 0)
				billboard.AlwaysOnTop = true
				local text =  Instance.new("TextLabel", billboard)         
				text.Size = UDim2.new(1,0,1,0)
				text.BackgroundTransparency = 1
				
				local newHighlight = Instance.new("Highlight", v.Character)
			newHighlight.Name = "ESPH"
			newHighlight.FillColor = Color3.fromRGB(196,196,196)
			end
		end
end

function module.ForceCreate(Part)
	local part = workspace:FindFirstChild(Part,true)
	if part then
		local billboard = Instance.new("BillboardGui", part)
		billboard.Size = UDim2.new(4,0,1,0)
		billboard.ExtentsOffset = Vector3.new(0, 2, 0)
		billboard.AlwaysOnTop = true
		local text =  Instance.new("TextLabel", billboard)         
		text.Size = UDim2.new(1,0,1,0)
		text.BackgroundTransparency = 1
		text.Text = Part
		text.TextColor3 = Color3.new(0.250980, 0.019608, 0.678431)
	end
end

function module.UpdateLabel()
	local Rep = game.ReplicatedStorage
	local PlayerRoles = Rep:FindFirstChild("GetPlayerData", true):InvokeServer()
	for i, v in game.Players:GetChildren() do
		if (v ~= game.Players.LocalPlayer and v.Character and v.Character:FindFirstChild("Head")) and v.Character:WaitForChild("Head"):FindFirstChild("BillboardGui") and v.Character:FindFirstChild("ESPH") then
			local text =  v.Character:WaitForChild("Head").BillboardGui.TextLabel
			local highlight : Highlight = v.Character:WaitForChild("ESPH")
			text.Text = v.Name
			if PlayerRoles[v.Name] then
				local ROLE = PlayerRoles[v.Name].Role
				if ROLE == "Sheriff" and IsAlive(PlayerRoles[v.Name]) then
					text.TextColor3 = Color3.fromRGB(0, 0, 225)
					highlight.FillColor = Color3.fromRGB(0,0,255)
				end

				if ROLE == "Murderer" and IsAlive(PlayerRoles[v.Name]) then
					text.TextColor3 = Color3.fromRGB(225, 0, 0)
					highlight.FillColor = Color3.fromRGB(255,0,0)
				end

				if ROLE == "Innocent" and IsAlive(PlayerRoles[v.Name]) then 
					text.TextColor3 = Color3.fromRGB(0, 225, 0)
					highlight.FillColor = Color3.fromRGB(0,255,0)
				end 

				if ROLE == "Hero" and IsAlive(PlayerRoles[v.Name]) then
					text.TextColor3 = Color3.fromRGB(255, 225, 0)
					highlight.FillColor = Color3.fromRGB(255,255,0)
				end
		end
		end
	end
end

function module.ClearAll()
	for i, v in game.Players:GetPlayers() do
		if (v ~= game.Players.LocalPlayer and v.Character and v.Character:FindFirstChild("Head")) and v.Character:WaitForChild("Head"):FindFirstChild("BillboardGui") and v.Parent then
			v.Character:WaitForChild("Head"):FindFirstChild("BillboardGui"):Destroy()
			if v.Character:FindFirstChild("ESPH") then
				v.Character:FindFirstChild("ESPH"):Destroy()
			end
		end
	end
end

return module

end;
};
G2L_MODULES[G2L["3c"]] = {
Closure = function()
    local script = G2L["3c"];local module = {}

local SilentAIMEnabled = false

module.toggle = function(toggle)
	SilentAIMEnabled = toggle
end

local function getRoles()
	local Rep = game.ReplicatedStorage
	local roles = Rep:FindFirstChild("GetPlayerData", true):InvokeServer()
	local ROLES
	
	for i,v in roles do
		if v.Role == "Murderer" then
			ROLES = game.Players:FindFirstChild(i)
		end
	end
	
	
	return ROLES
end

local lastVelocity = Vector3.zero
local Stats = game:GetService("Stats")
local RunService = game:GetService("RunService")
local Players = game:GetService("Players")
local player = Players.LocalPlayer

local FIXED_PING = 0.06
local VELOCITY_CLAMP = 80
local SMOOTHING = 0.7

local function predictPosition(root, humanoid)
	local velocity = root.AssemblyLinearVelocity

	velocity = lastVelocity:Lerp(velocity, SMOOTHING)
	lastVelocity = velocity

	if velocity.Magnitude > VELOCITY_CLAMP then
		velocity = velocity.Unit * VELOCITY_CLAMP
	end

	local yMultiplier = 0.15
	local state = humanoid:GetState()

	if state == Enum.HumanoidStateType.Jumping then
		yMultiplier = 0.25
	elseif state == Enum.HumanoidStateType.Freefall then
		yMultiplier = 0.05
	end

	return root.Position + Vector3.new(
		velocity.X * FIXED_PING,
		velocity.Y * FIXED_PING * yMultiplier,
		velocity.Z * FIXED_PING
	)
end


function module.Shoot()
	local target = getRoles()
	if not target or not target.Character then return end

	local root = target.Character:FindFirstChild("HumanoidRootPart")
	local humanoid = target.Character:FindFirstChildOfClass("Humanoid")
	local myChar = player.Character
	if not root or not humanoid or not myChar then return end

	local hand = myChar:WaitForChild("RightHand")

	local predictedPos = predictPosition(root, humanoid)

	local args = {
		hand.CFrame,
		CFrame.new(predictedPos)
	}

	local backpackGun = player.Backpack:FindFirstChild("Gun")
	if backpackGun then
		backpackGun.Parent = myChar
	end

	local gun = myChar:FindFirstChild("Gun")
	if gun then
		gun.Shoot:FireServer(unpack(args))
	end
end


return module

end;
};
