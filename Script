G2L_MODULES[G2L["34"]] = {
Closure = function()
    local script = G2L["34"];local module = {}

local Players = game:GetService("Players")
local RunService = game:GetService("RunService")
local UIS = game:GetService("UserInputService")

local player = Players.LocalPlayer
local camera = workspace.CurrentCamera
local gui = player:WaitForChild("PlayerGui")

local character = player.Character or player.CharacterAdded:Wait()
local humanoid = character:WaitForChild("Humanoid")
local hrp = character:WaitForChild("HumanoidRootPart")

local flying = false
local speed = 90
local bv
local bg
local input = {W=0,A=0,S=0,D=0}
local mobileInput = Vector3.zero
local isMobile = false
local lastSafePosition
local collisionCooldown = 0

if UIS.TouchEnabled and not UIS.KeyboardEnabled then
	isMobile = true
end



local function toggle()
	flying = not flying
	if flying then
		humanoid.PlatformStand = true
		humanoid.AutoRotate = false
		humanoid:ChangeState(Enum.HumanoidStateType.Freefall)


		bv = Instance.new("BodyVelocity")
		bv.MaxForce = Vector3.new(1e20,1e20,1e20)
		bv.Velocity = Vector3.zero
		bv.Parent = hrp


		bg = Instance.new("BodyGyro")
		bg.MaxTorque = Vector3.new(1e20,1e20,1e20)
		bg.P = 10000
		bg.D = 1000
		bg.Parent = hrp

		lastSafePosition = hrp.Position
		collisionCooldown = 0
	else
		if bv then bv:Destroy() bv = nil end
		if bg then bg:Destroy() bg = nil end
		humanoid.PlatformStand = false
		humanoid.AutoRotate = true
	end
end

UIS.InputBegan:Connect(function(i,gp)
	if gp then return end
	if i.KeyCode == Enum.KeyCode.W then input.W = 1 end
	if i.KeyCode == Enum.KeyCode.S then input.S = 1 end
	if i.KeyCode == Enum.KeyCode.A then input.A = 1 end
	if i.KeyCode == Enum.KeyCode.D then input.D = 1 end
	if i.KeyCode == Enum.KeyCode.G then
		toggle()
	end
end)

UIS.InputEnded:Connect(function(i)
	if i.KeyCode == Enum.KeyCode.W then input.W = 0 end
	if i.KeyCode == Enum.KeyCode.S then input.S = 0 end
	if i.KeyCode == Enum.KeyCode.A then input.A = 0 end
	if i.KeyCode == Enum.KeyCode.D then input.D = 0 end
end)

RunService.RenderStepped:Connect(function()
	if not flying or not bv or not bg then return end

	if collisionCooldown > 0 then
		collisionCooldown = collisionCooldown - 1
	end

	local camCF = camera.CFrame
	local dir

	if isMobile then
		if humanoid.MoveDirection.Magnitude > 0 then
			dir = camCF.RightVector * mobileInput.X + camCF.LookVector * mobileInput.Z
		else
			dir = Vector3.zero
		end
	else
		dir = camCF.LookVector * (input.W - input.S) + camCF.RightVector * (input.D - input.A)
	end

	if dir.Magnitude > 0 then
		dir = dir.Unit
	end

	local nextPosition = hrp.Position + dir * speed * 0.016
	local raycastParams = RaycastParams.new()
	raycastParams.FilterDescendantsInstances = {character}
	raycastParams.FilterType = Enum.RaycastFilterType.Exclude

	local raycastResult = workspace:Raycast(hrp.Position, nextPosition - hrp.Position, raycastParams)

	if raycastResult and raycastResult.Distance < 2 then
		if collisionCooldown == 0 then
			bv.Velocity = Vector3.zero

			local bounceDir = (hrp.Position - raycastResult.Position).Unit
			if bounceDir.Magnitude > 0 then
				bv.Velocity = bounceDir * speed * 0.3
			end

			collisionCooldown = 30
		end
	else
		bv.Velocity = dir * speed


		if dir.Magnitude > 0.1 then
			lastSafePosition = hrp.Position
		end
	end


	local lookDir = Vector3.new(camCF.LookVector.X, camCF.LookVector.Y, camCF.LookVector.Z).Unit
	if lookDir.Magnitude > 0 then
		bg.CFrame = CFrame.new(hrp.Position, hrp.Position + lookDir)
	end
end)


module.Toggle = toggle

return module

end;
};
